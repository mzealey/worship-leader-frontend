{
  "name": "worship-leader",
  "private": true,
  "version": "10.1.1",
  "type": "module",
  "description": "Worship Leader app",
  "workspaces": [
    "abc2svg",
    "wa-sqlite"
  ],
  "browserslist": [
    "last 2 versions",
    "since 2019",
    "> 0.2%",
    "not dead",
    "not ie <= 11"
  ],
  "scripts": {
    "lint": "yarn lint:prettier && yarn lint:eslint:fix && yarn lint:typescript && yarn lint:typescript:spec",
    "lint:prettier": "prettier --write {spec,src}/**/*.{js,jsx,ts,tsx,css,scss} *.{js,mjs,ts,jsx,tsx} --experimental-cli",
    "lint:eslint": "eslint src/ spec/",
    "lint:eslint:fix": "eslint --fix src/ spec/",
    "lint:typescript": "tsc --noEmit",
    "lint:typescript:spec": "tsc --noEmit --project tsconfig.spec.json",
    "dev": "vite --mode www",
    "test": "yarn test:unit && yarn test:browser",
    "test:unit;watch": "vitest --dir spec",
    "test:unit": "vitest run --dir spec",
    "test:coverage": "vitest run --dir spec --coverage",
    "test:browser": "playwright test",
    "test:browser:ui": "playwright test --ui",
    "test:browser:headed": "playwright test --headed",
    "build": "concurrently yarn:build:test:*",
    "build:prod": "concurrently yarn:build:prod:*",
    "build:test:www": "vite build --mode www --outDir build/www",
    "build:prod:www": "vite build --mode www --outDir ../../root/static/offline --emptyOutDir",
    "build:test:chrome": "vite build --mode chrome --outDir build/chrome",
    "build:prod:chrome": "vite build --mode chrome --outDir build/chrome",
    "build:test:edge": "vite build --mode edge --outDir build/edge",
    "build:prod:edge": "vite build --mode edge --outDir build/edge",
    "build:test:editor": "vite build --mode editor --outDir build/editor",
    "build:prod:editor": "vite build --mode editor --outDir ../../root/static/editor/js/",
    "build:test:phonegap": "vite build --mode phonegap --outDir build/phonegap",
    "build:prod:phonegap": "vite build --mode phonegap --outDir ../phonegap/www --emptyOutDir"
  },
  "dependencies": {
    "@fingerprintjs/botd": "^1.9.1",
    "classlist-polyfill": "^1.2.0",
    "comlink": "^4.4.2",
    "core-js": "^2.6.1",
    "grapheme-splitter": "^1.0.4",
    "jquery": "^1.12.4",
    "jquery-ui": "^1.12.1",
    "jquery-ui-touch-punch": "^0.2.3",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.21",
    "raty-js": "^2.9.0",
    "stacktrace-js": "^2.0.0"
  },
  "devDependencies": {
    "@originjs/vite-plugin-commonjs": "^1.0.3",
    "@playwright/test": "^1.56.0",
    "@types/jquery": "^3.5.33",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^24.7.2",
    "@vitejs/plugin-legacy": "^7.2.1",
    "@vitest/coverage-v8": "^3.2.4",
    "concurrently": "^9.2.1",
    "eslint": "^9.37.0",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.4.0",
    "jsdom": "^27.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "sass-embedded": "^1.83.0",
    "terser": "^5.37.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.3.0",
    "vite-multiple-assets": "^2.2.5",
    "vite-plugin-circular-dependency": "^0.5.0",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-pwa": "^1.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4"
  },
  "author": "Mark Zealey",
  "license": "Private",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
